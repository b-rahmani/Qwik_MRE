{
    "name": "qwik",
    "engines": {
        "node": ">=15.0.0"
    },
    "private": true,
    "scripts": {
        "build": "qwik build",
        "build.client": "vite build",
        "build.preview": "vite build --ssr src/entry.preview.tsx",
        "build.server": "vite build -c adapters/express/vite.config.js",
        "build.types": "tsc --incremental --noEmit",
        "deploy": "node server/entry.express",
        "dev": "vite --mode ssr --clearScreen false",
        "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
        "fmt": "prettier --write .",
        "fmt.check": "prettier --check .",
        "lint": "eslint \"src/**/*.ts*\"",
        "preview": "qwik build preview && vite preview",
        "serve": "node server/entry.express",
        "start": "vite --mode ssr",
        "qwik": "qwik"
    },
    "devDependencies": {
        "@auth/core": "^0.18.0",
        "@builder.io/partytown": "^0.8.1",
        "@builder.io/qwik": "^1.2.15",
        "@builder.io/qwik-auth": "~0.1.3",
        "@builder.io/qwik-city": "^1.2.15",
        "@tailwindcss/typography": "^0.5.10",
        "@types/compression": "^1.7.4",
        "@types/cors": "^2.8.15",
        "@types/eslint": "^8.44.6",
        "@types/express": "^4.17.20",
        "@types/node": "^20.8.9",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "autoprefixer": "^10.4.16",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "escodegen": "^2.1.0",
        "eslint": "^8.52.0",
        "eslint-plugin-qwik": "^1.2.15",
        "estraverse": "^5.3.0",
        "express": "^4.18.2",
        "nightly-esprima": "^2021.8.30",
        "node-fetch": "^3.3.1",
        "postcss": "^8.4.31",
        "prettier": "^3.0.2",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2",
        "undici": "^5.27.0",
        "vite": "^4.5.0",
        "vite-tsconfig-paths": "^4.2.1"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "chart.js": "^4.4.0",
        "glider-js": "^1.7.8",
        "leaflet": "^1.9.4",
        "prismjs": "^1.29.0",
        "swiper": "^10.3.1",
        "tailwind-merge": "^1.14.0"
    }
}
